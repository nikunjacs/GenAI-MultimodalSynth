from backend.model_handler import generate_multimodal_output
import asyncio

async def test():
    with open("../data/sample_texts/sample.txt", "rb") as f_text, \
         open("../data/sample_images/sample.jpg", "rb") as f_img, \
         open("../data/sample_audio/sample.wav", "rb") as f_audio:
        output = await generate_multimodal_output(f_text, f_img, f_audio)
    print(output)

asyncio.run(test())
